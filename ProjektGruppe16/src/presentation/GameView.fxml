<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ProgressBar?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<VBox prefHeight="629.0" prefWidth="867.0" xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1" fx:controller="presentation.GameViewController">
  <children>
    <AnchorPane maxHeight="-1.0" maxWidth="-1.0" prefHeight="-1.0" prefWidth="-1.0" VBox.vgrow="ALWAYS">
         <children>
            <Label fx:id="label" layoutX="59.0" layoutY="407.0" text="Label" />
            <Button layoutX="780.0" layoutY="543.0" mnemonicParsing="false" onAction="#goEast" text="Button" />
            <Button layoutX="728.0" layoutY="493.0" mnemonicParsing="false" onAction="#goNorth" text="Button" />
            <Button layoutX="676.0" layoutY="543.0" mnemonicParsing="false" onAction="#goWest" text="Button" />
            <Button layoutX="728.0" layoutY="583.0" mnemonicParsing="false" onAction="#goSouth" text="Button" />
            <Button layoutX="56.0" layoutY="530.0" mnemonicParsing="false" onAction="#attack" text="Attack" />
            <Button layoutX="164.0" layoutY="531.0" mnemonicParsing="false" text="Use" />
            <Button layoutX="261.0" layoutY="531.0" mnemonicParsing="false" text="Talk" />
            <Button layoutX="156.0" layoutY="570.0" mnemonicParsing="false" text="Pickup" />
            <Button layoutX="733.0" layoutY="24.0" mnemonicParsing="false" onAction="#saveGame" text="Save" />
            <Button layoutX="796.0" layoutY="24.0" mnemonicParsing="false" onAction="#loadGame" text="Load" />
            <Button layoutX="59.0" layoutY="570.0" mnemonicParsing="false" text="Drop" />
            <Button layoutX="26.0" layoutY="24.0" mnemonicParsing="false" text="Exit" />
            <Button layoutX="389.0" layoutY="529.0" mnemonicParsing="false" onAction="#launchGame" prefHeight="54.0" prefWidth="89.0" text="Start" />
            <Label fx:id="health" layoutX="658.0" layoutY="65.0" text="Label" />
            <ProgressBar fx:id="healthBar" layoutX="656.0" layoutY="82.0" prefWidth="200.0" progress="1.0" style="-fx-accent: red;" />
         </children>
    </AnchorPane>
  </children>
</VBox>
